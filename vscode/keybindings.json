// Place your key bindings in this file to override the defaults
[
    {
        "command": "vscode-neovim.compositeEscape1",
        "key": "j",
        "when": "neovim.mode == insert && inputFocus",
        "args": "j"
    },
    {
        "key": "tab",
        "command": "selectNextSuggestion",
        "when": "suggestWidgetMultipleSuggestions && suggestWidgetVisible && textInputFocus"
    },
    {
        "key": "down",
        "command": "-selectNextSuggestion",
        "when": "suggestWidgetMultipleSuggestions && suggestWidgetVisible && textInputFocus"
    },
    {
        "key": "shift+tab",
        "command": "selectPrevSuggestion",
        "when": "suggestWidgetMultipleSuggestions && suggestWidgetVisible && textInputFocus"
    },
    {
        "key": "up",
        "command": "-selectPrevSuggestion",
        "when": "suggestWidgetMultipleSuggestions && suggestWidgetVisible && textInputFocus"
    },
    {
        "key": "enter",
        "command": "acceptAlternativeSelectedSuggestion",
        "when": "suggestWidgetMultipleSuggestions && textInputFocus && textInputFocus"
    },
    {
        "key": ", f",
        "command": "workbench.action.quickOpen",
        "when": "neovim.mode != insert"
    },
    {
        "key": "cmd+p",
        "command": "-workbench.action.quickOpen"
    },
    {
        "key": "space 1",
        "command": "workbench.action.openEditorAtIndex1",
        "when": "!inputFocus || (editorFocus && neovim.mode == normal)"
    },
    {
        "key": "ctrl+1",
        "command": "-workbench.action.openEditorAtIndex1"
    },
    {
        "key": "space 2",
        "command": "workbench.action.openEditorAtIndex2",
        "when": "!inputFocus || (editorFocus && neovim.mode == normal)"
    },
    {
        "key": "ctrl+2",
        "command": "-workbench.action.openEditorAtIndex2"
    },
    {
        "key": "space 3",
        "command": "workbench.action.openEditorAtIndex3",
        "when": "!inputFocus || (editorFocus && neovim.mode == normal)"
    },
    {
        "key": "ctrl+3",
        "command": "-workbench.action.openEditorAtIndex3"
    },
    {
        "key": "space 4",
        "command": "workbench.action.openEditorAtIndex4",
        "when": "!inputFocus || (editorFocus && neovim.mode == normal)"
    },
    {
        "key": "ctrl+4",
        "command": "-workbench.action.openEditorAtIndex4"
    },
    {
        "key": "space 5",
        "command": "workbench.action.openEditorAtIndex5",
        "when": "!inputFocus || (editorFocus && neovim.mode == normal)"
    },
    {
        "key": "ctrl+5",
        "command": "-workbench.action.openEditorAtIndex5"
    },
    {
        "key": "space 6",
        "command": "workbench.action.openEditorAtIndex6",
        "when": "!inputFocus || (editorFocus && neovim.mode == normal)"
    },
    {
        "key": "ctrl+6",
        "command": "-workbench.action.openEditorAtIndex6"
    },
    {
        "key": "space 7",
        "command": "workbench.action.openEditorAtIndex7",
        "when": "!inputFocus || (editorFocus && neovim.mode == normal)"
    },
    {
        "key": "ctrl+7",
        "command": "-workbench.action.openEditorAtIndex7"
    },
    {
        "key": "space 8",
        "command": "workbench.action.openEditorAtIndex8",
        "when": "!inputFocus || (editorFocus && neovim.mode == normal)"
    },
    {
        "key": "ctrl+8",
        "command": "-workbench.action.openEditorAtIndex8"
    },
    {
        "key": "space 9",
        "command": "workbench.action.openEditorAtIndex9",
        "when": "!inputFocus || (editorFocus && neovim.mode == normal)"
    },
    {
        "key": "ctrl+9",
        "command": "-workbench.action.openEditorAtIndex9"
    },
    {
        "key": "space q",
        "command": "workbench.action.closeActiveEditor",
        "when": "!inputFocus || (editorFocus && neovim.mode == normal)"
    },
    {
        "key": "cmd+w",
        "command": "-workbench.action.closeActiveEditor"
    },
    {
        "key": "space w",
        "command": "workbench.action.files.save",
        "when": "neovim.mode == normal && editorFocus"
    },
    {
        "key": "cmd+s",
        "command": "-workbench.action.files.save"
    },
    {
        "key": "ctrl+l",
        "command": "workbench.action.focusRightGroup"
    },
    {
        "key": "cmd+k cmd+right",
        "command": "-workbench.action.focusRightGroup"
    },
    {
        "key": "ctrl+l",
        "command": "-vscode-neovim.send-cmdline",
        "when": "neovim.mode == 'cmdline_insert' || neovim.mode == 'cmdline_normal' || neovim.mode == 'cmdline_replace'"
    },
    {
        "key": "ctrl+l",
        "command": "-vscode-neovim.send",
        "when": "!inputFocus && neovim.ctrlKeysNormal && neovim.init && neovim.mode != 'insert'"
    },
    {
        "key": "ctrl+l",
        "command": "-notebook.centerActiveCell",
        "when": "notebookEditorFocused"
    },
    {
        "key": "ctrl+h",
        "command": "workbench.action.focusLeftGroup",
        "when": "neovim.mode != insert"
    },
    {
        "key": "cmd+k cmd+left",
        "command": "-workbench.action.focusLeftGroup"
    },
    {
        "key": "ctrl+s c",
        "command": "workbench.action.terminal.new",
        "when": "terminalProcessSupported || terminalWebExtensionContributedProfile"
    },
    {
        "key": "ctrl+shift+`",
        "command": "-workbench.action.terminal.new",
        "when": "terminalProcessSupported || terminalWebExtensionContributedProfile"
    },
    {
        "key": "alt+k",
        "command": "workbench.action.terminal.focusNext",
        "when": "terminalFocus && terminalHasBeenCreated && !terminalEditorFocus || terminalFocus && terminalProcessSupported && !terminalEditorFocus"
    },
    {
        "key": "shift+cmd+]",
        "command": "-workbench.action.terminal.focusNext",
        "when": "terminalFocus && terminalHasBeenCreated && !terminalEditorFocus || terminalFocus && terminalProcessSupported && !terminalEditorFocus"
    },
    {
        "key": "alt+j",
        "command": "workbench.action.terminal.focusPrevious",
        "when": "terminalFocus && terminalHasBeenCreated && !terminalEditorFocus || terminalFocus && terminalProcessSupported && !terminalEditorFocus"
    },
    {
        "key": "shift+cmd+[",
        "command": "-workbench.action.terminal.focusPrevious",
        "when": "terminalFocus && terminalHasBeenCreated && !terminalEditorFocus || terminalFocus && terminalProcessSupported && !terminalEditorFocus"
    },
    {
        "key": "g h",
        "command": "editor.action.referenceSearch.trigger",
        "when": "neovim.mode == normal && editorFocus"
    },
    {
        "key": "] g",
        "command": "editor.action.marker.next",
        "when": "neovim.mode == normal && editorFocus"
    },
    {
        "key": "alt+f8",
        "command": "-editor.action.marker.next",
        "when": "editorFocus"
    },
    {
        "key": "[ g",
        "command": "editor.action.marker.prev",
        "when": "neovim.mode == normal && editorFocus"
    },
    {
        "key": "shift+alt+f8",
        "command": "-editor.action.marker.prev",
        "when": "editorFocus"
    },
    {
        "key": "alt+h",
        "command": "workbench.action.focusFirstEditorGroup"
    },
    {
        "key": "cmd+1",
        "command": "-workbench.action.focusFirstEditorGroup"
    },
    {
        "key": "alt+l",
        "command": "workbench.action.terminal.focus"
    },
    {
        "key": "ctrl+s ,",
        "command": "workbench.action.terminal.rename"
    },
    {
        "key": "g g",
        "command": "cursorTop",
        "when": "textInputFocus && neovim.mode == 'normal'"
    },
    {
        "key": "cmd+up",
        "command": "-cursorTop",
        "when": "textInputFocus"
    }
]