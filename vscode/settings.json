{
  "editor.suggestSelection": "first",
  "vscode-neovim.neovimExecutablePaths.darwin": "/usr/local/bin/nvim",
  "vscode-neovim.neovimInitVimPaths.darwin": "$HOME/dotfiles/nvim/init.lua",
  "vscode-neovim.logOutputToConsole": true,
  "vscode-neovim.logLevel": "debug",
  "files.exclude": {
    "**/.classpath": true,
    "**/.project": true,
    "**/.settings": true,
    "**/.factorypath": true
  },
  "workbench.startupEditor": "none",
  "extensions.experimental.affinity": {
    "asvetliakov.vscode-neovim": 1
  },
  "editor.fontFamily": "'HackGen35 Console NF'",
  "update.showReleaseNotes": false,
  "editor.renderWhitespace": "all",
  "editor.cursorStyle": "block",
  "git.openRepositoryInParentFolders": "always",
  "boot-java.rewrite.reconcile": true,
  "git.autofetch": true,
  "editor.acceptSuggestionOnEnter": "off",
  "security.workspace.trust.untrustedFiles": "open",
  "java.format.settings.url": "~/develop/java/formatter.xml",
  "java.format.settings.profile": "Style",
  "editor.formatOnSave": true,
  "workbench.iconTheme": "material-icon-theme",
  "workbench.colorTheme": "Default Dark+ Experimental",
  "editor.semanticTokenColorCustomizations": {
    "enabled": true
  },
  "editor.fontSize": 13,
  "editor.inlayHints.enabled": "off",
  "editor.lightbulb.enabled": false,
  "editor.renderFinalNewline": "off",
  "terminal.integrated.defaultLocation": "editor",
  "editor.minimap.enabled": false,
  "editor.wordWrap": "on",
  "redhat.telemetry.enabled": false,
  "editor.suggest.selectionMode": "whenTriggerCharacter",
  "terminal.integrated.copyOnSelection": true,
  "terminal.integrated.enableMultiLinePasteWarning": false,
  "window.restoreWindows": "none",
  "terminal.integrated.persistentSessionReviveProcess": "never",
  "terminal.integrated.persistentSessionScrollback": 10000,
  "workbench.localHistory.enabled": false,
  "workbench.editor.restoreViewState": false,
  "files.hotExit": "off"
}